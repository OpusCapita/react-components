{
  "name": "@opuscapita/react-menus",
  "version": "2.0.0",
  "description": "Shared menu components",
  "main": "index.js",
  "scripts": {
    "local": "npm run setup-consul ; NODE_ENV=development nodemon --watch src/server src/local/index.js",
    "build:client": "${NODE_PATH}/node_modules/.bin/webpack -p --config ./webpack.production.config.js --profile --colors --bail",
    "setup-consul": "curl -X PUT -d ${MYSQL_DATABASE} http://consul:8500/v1/kv/isodata/db-init/database ; curl -X PUT -d 'root' http://consul:8500/v1/kv/isodata/db-init/user ; curl -X PUT -d ${MYSQL_ROOT_PASSWORD} http://consul:8500/v1/kv/isodata/db-init/password "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpusCapita/react-menus.git"
  },
  "author": "Christian Drengenberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpusCapita/react-menus/issues"
  },
  "homepage": "https://github.com/OpusCapita/react-menus#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.0",
    "nodemon": "^1.11.0",
    "ocbesbn-web-init": "^2.2.3",
    "@opuscapita/i18n": "1.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "superagent": "^3.6.0",
    "superagent-bluebird-promise": "^4.2.0",
    "svg-react-loader": "^0.4.4",
    "webpack": "^3.5.5"
  }
}
